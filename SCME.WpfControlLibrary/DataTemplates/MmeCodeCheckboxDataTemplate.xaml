<UserControl x:Class="SCME.WpfControlLibrary.DataTemplates.MmeCodeCheckboxDataTemplate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:viewModels="clr-namespace:SCME.WpfControlLibrary.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance viewModels:MmeCodeCheckboxDataTemplateVm }">
    <CheckBox Content="{Binding Name}" IsChecked="{Binding IsCheckedNewValue}"></CheckBox>
</UserControl>
